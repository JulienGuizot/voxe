@font-face {
    font-family: 'bebasregular';
    src: url('<%= asset_path('bebas_regular/BEBAS___-webfont.eot') %>');
    src: url('<%= asset_path('bebas_regular/BEBAS___-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('bebas_regular/BEBAS___-webfont.woff') %>') format('woff'),
         url('<%= asset_path('bebas_regular/BEBAS___-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('bebas_regular/BEBAS___-webfont.svg#bebasregular') %>') format('svg');
    font-weight: normal;
    font-style: normal;
}
